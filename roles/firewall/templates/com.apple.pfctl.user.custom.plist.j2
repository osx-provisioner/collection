<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.apple.pfctl.user.custom</string>
    <key>WorkingDirectory</key>
    <string>/var/run</string>
    <key>LaunchOnlyOnce</key>
    <true/>
    <key>Program</key>
    <string>/usr/local/bin/pf-configure.sh</string>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>{{ firewall_service_stdout_log_file }}</string>
    <key>StandardErrorPath</key>
    <string>{{ firewall_service_stderr_log_file }}</string>
  </dict>
</plist>